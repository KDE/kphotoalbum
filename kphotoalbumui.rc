<!DOCTYPE kpartgui>
<kpartgui name="kphotoalbum" version="34">
  <MenuBar>
    <Menu name="file">
      <Action name="exportHTML"/>
      <Action name="import"/>
      <Action name="export"/>
    </Menu>
    <Menu name="edit" >
      <Action name="copySelectedURLs" />
      <Action name="deleteSelected" />
      <Action name="removeTokens" />
      <Separator/>
      <Action name="rotateLeft" />
      <Action name="rotateRight" />
      <Separator/>
      <Action name="oneProp"/>
      <Action name="allProp"/>
    </Menu>
    <Menu name="view" >
      <text>&amp;View</text>
      <Action name="viewImages" />
      <Action name="viewImagesNewWindow" />
      <Action name="runSlideShow"/>
      <Action name="runRandomizedSlideShow"/>
      <Separator/>
      <Action name="jumpToContext"/>
      <Action name="limitToMarked" />
      <Separator/>
      <Action name="sortImages" />
      <Action name="orderIncr"/>
      <Action name="orderDecr"/>
      <Separator/>
      <Menu name="privacy">
        <text>Privacy</text>
        <Action name="lockToDefaultScope"/>
        <Action name="unlockFromDefaultScope"/>
        <Action name="changeScopePasswd" />
        <separator/>
        <Action name="setDefaultScopeNegative" />
        <Action name="setDefaultScopePositive" />
      </Menu>
    </Menu>
    <Menu name="maintenance">
      <text>&amp;Maintenance</text>
      <Action name="findUnavailableImages"/>
      <Action name="findImagesWithInvalidDate"/>
      <Action name="findImagesWithChangedMD5Sum"/>
      <separator/>
      <Action name="rebuildMD5s"/>
      <Action name="rescan"/>
      <Action name="reReadExifInfo" />
      <Action name="writeExifInfo" />
      <Action name="convertBackend" />
      <Separator/>
      <Action name="removeAllThumbs"/>
      <Action name="buildThumbs"/>
    </Menu>
    <Menu name="plugins">
      <text>Plugins</text>
      <Menu name="exportplugin">       
        <text>Export</text>
        <ActionList name="export_actions"/>
      </Menu>
      <Menu name="importplugin">       
        <text>Import</text>
        <ActionList name="import_actions"/>
      </Menu>
      <Menu name="imagesplugins">
      <text>Images</text>
         <ActionList name="image_actions"/>  
      </Menu>
      <Menu name="batch_plugins">
        <text>Batch Plugins</text>
        <ActionList name="batch_actions"/>
      </Menu>
      <Menu name="tool_plugins">
        <text>Tools</text>
        <ActionList name="tool_actions"/>
      </Menu>
   </Menu>
   <Menu name="settings">
     <Action name="readdAllMessages"/>
     <Action name="showToolTipOnImages"/>
   </Menu>
   <Menu name="help">
     <Action name="runDemo"/>
     <Action name="runSurvey"/>
     <Action name="donate"/>
     <Action name="features"/>
   </Menu>
  </MenuBar>

  <ToolBar name="mainToolBar">
    <text>Main Toolbar</text>
    <Action name="go_back"/>
    <Action name="go_forward"/>
    <Action name="view_redisplay"/>
    <Action name="runSlideShow"/>
    <Action name="configureView"/>
  </ToolBar>


</kpartgui>
